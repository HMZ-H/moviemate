{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Backend: Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/backend/main.go",
      "cwd": "${workspaceFolder}/backend",
      "envFile": "${workspaceFolder}/backend/.env"
    },
    {
      "name": "Backend: Attach",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    }
  ]
}